{
  "name": "ftgo-graphql-server",
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "compile": "tsc",
    "start": "yarn compile && node ./dist/src/index.js",
    "watch": "concurrently --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"gql-gen --config codegen.ts --watch 'src/**/graphql.schema'\" \"tsc -w\" \"nodemon ./dist/src/index.js\"",
    "codegen": "gql-gen --config codegen.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@parcel/watcher": "^2.2.0",
    "@types/body-parser": "^1",
    "@types/cors": "^2",
    "@types/express": "^4",
    "@types/google-protobuf": "^3",
    "@types/node": "^20.4.8",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^2",
    "typescript": "~5"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@grpc/grpc-js": "^1.9.0",
    "@jangjunha/ftgo-proto": "^0.1.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-protobuf": "^3.21.2",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2"
  }
}
